                </ui:Label>
                <ui:Label text="Defender&#10;" name="Weapon">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.weapon" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
