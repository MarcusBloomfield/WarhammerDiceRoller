                <ui:Label text="Attacks&#10;" name="Attacks">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.attacks" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                    </Bindings>
                </ui:Label>
                <ui:Label text="Hits&#10;" name="Hits">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.hits" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
