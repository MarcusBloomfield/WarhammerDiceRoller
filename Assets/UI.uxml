                        <ui:Label text="Label" name="Toughness">
                            <Bindings>
                                <ui:DataBinding property="text" data-source-path="selectedDefence.toughness" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                            </Bindings>
                        </ui:Label>
                        <ui:Label text="Label" name="Save">
                            <Bindings>
                                <ui:DataBinding property="text" data-source-path="selectedDefence.save" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                            </Bindings>
                        </ui:Label>
                        <ui:Label text="Label" name="InvulnerableSave">
                            <Bindings>
                                <ui:DataBinding property="text" data-source-path="selectedDefence.invulnerableSave" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                            </Bindings>
                        </ui:Label>
                    </ui:VisualElement>
                </ui:VisualElement>
                <ui:Button text="Roll" name="Roll" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
