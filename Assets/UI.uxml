                <ui:Label text="Owner:&#10;" name="Owner" />
                <ui:Label text="Defender:&#10;" name="Defender" />
                <ui:Label text="Weapon:" name="Weapon" />
                <ui:Label text="Attacks:&#10;" name="Attacks" />
                <ui:Label text="Hits:&#10;" name="Hits" />
                <ui:Label text="Wounds:&#10;" name="Wounds" />
                <ui:Label text="Saves:&#10;" name="Saves" />
                <ui:Label text="Damage:&#10;" name="Damage" />
            </ui:VisualElement>
            <ui:VisualElement name="Results" style="flex-grow: 1; align-items: flex-start; width: auto;">
                <ui:Label text="Owner&#10;" name="Owner">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.owner" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                    </Bindings>
                </ui:Label>
                <ui:Label text="Defender&#10;" name="Defender">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.defender" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
