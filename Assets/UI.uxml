                <ui:Label text="Wounds&#10;" name="Wounds">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.wounds" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                    </Bindings>
                </ui:Label>
                <ui:Label text="Saves&#10;" name="Saves">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.saves" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                    </Bindings>
                </ui:Label>
                <ui:Label text="Damage&#10;" name="Damage">
                    <Bindings>
                        <ui:DataBinding property="text" data-source-path="results.damage" data-source-type="WarhammerDice, Assembly-CSharp" binding-mode="ToTarget" />
                    </Bindings>
                </ui:Label>
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
